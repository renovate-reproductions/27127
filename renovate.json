{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "logLevelRemap": [
    {
      "matchMessage": "PullRequests*",
      "newLogLevel": "info"
    }
  ],
  "regexManagers": [
    {
      "fileMatch": ["\\.yml$"],
      "matchStrings": ["VAULT_VERSION *: *\"(?<currentValue>.+?)\""],
      "depNameTemplate": "hashicorp/vault",
      "datasourceTemplate": "github-releases"
    }
  ]
}
